{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft.AspNetCore": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "File",
				"path": "D:\\home\\LogFiles\\rawLogs\\joserras.txt",
				"rollingInterval": "Month",
				"shared": "true",
				"flushToDiskInterval": "0:00:02"
			}
		]
	},

	"AllowedHosts": "*",
	"CreateBalance": "true"

}